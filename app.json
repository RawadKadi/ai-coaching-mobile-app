{
    "expo": {
        "name": "bolt-expo-nativewind",
        "slug": "bolt-expo-nativewind",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "coachingapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.coaching.app"
        },
        "android": {
            "package": "com.coaching.app",
            "intentFilters": [
                {
                    "action": "VIEW",
                    "data": [
                        {
                            "scheme": "coachingapp"
                        }
                    ],
                    "category": [
                        "BROWSABLE",
                        "DEFAULT"
                    ]
                }
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "single",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-font",
            "expo-web-browser"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}